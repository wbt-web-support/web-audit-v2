# Supabase Configuration
# Copy this file to .env.local and fill in your actual Supabase credentials

# Your Supabase project URL
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url_here

# Your Supabase anon/public key
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Your Supabase service role key (for admin operations)
NEXT_PUBLIC_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Scraping API Configuration
# The endpoint for the web scraping service (must use HTTPS in production)
NEXT_PUBLIC_SCRAPER_API_ENDPOINT=https://your-scraper-api-endpoint.com/scrap